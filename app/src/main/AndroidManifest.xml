<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.broccoli">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_brocolli_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_brocolli_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Broccoli">
        <activity android:name=".SplashScreen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".PostRegisted" />
        <activity android:name=".Register" />
        <activity android:name=".SuccessRegister" />
        <activity android:name=".PreRegister">

        </activity>
    </application>

</manifest>